#GIT
alias gs='git status '
alias ga='git add '
alias gb='git branch '
alias gc='git commit'
alias gd='git diff'
# dont fuck with golang -> alias go='git checkout '
alias gk='gitk --all&'
alias gx='gitx --all'

function gcp(){
    git commit -a -m $1;
    git push;
}


alias gstl='git stash list'
alias gstshow='git stash show stash^{/$*}'
alias gstapply='git stash apply stash^{/$*}'

