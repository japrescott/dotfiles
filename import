
#Get all Alias Sources
function src() {
  local file
  if [[ "$1" ]]; then
    source "$HOME/dotfiles/source/$1.sh"
  else
    for file in ~/dotfiles/source/*; do
      source "$file"
    done
  fi
}

# import all the aliases
src

#pimp the prompt
source ~/dotfiles/.shell_prompt.sh


#Load up all modules
. ~/dotfiles/modules/z/z.sh
